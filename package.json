    "create:local": "graph create rysk-local --node http://127.0.0.1:8020",
    "deploy:local": "graph deploy rysk-local --debug-fork QmQzNyEVieMgWkQuFtmk8QhwX6RDZEWreRknWzX5KJRukA --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
