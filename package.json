{
  "name": "rysktestnet",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare:arbitrum-goerli": "mustache config/arbitrum-goerli.json subgraph.template.yaml > subgraph.yaml",
    "deploy:arbitrum-goerli": "npm run prepare:arbitrum-goerli && graph deploy --node https://api.thegraph.com/deploy/ rysk-finance/rysk-goerli",
    "prepare:arbitrum": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
    "deploy:arbitrum": "npm run prepare:arbitrum && graph deploy --node https://api.thegraph.com/deploy/ rysk-finance/rysk",
    "prepare:arbitrum-local": "mustache config/arbitrum-local.json subgraph.template.yaml > subgraph.yaml",
    "create:local": "graph create rysk-local --node http://127.0.0.1:8020",
    "deploy:local": "graph deploy rysk-local --debug-fork QmWJcbnVSHr66trpwdarRpjnbosohjDxV6KjT5jUsbNfYX --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.30.4",
    "@graphprotocol/graph-ts": "0.27.0"
  },
  "devDependencies": {
    "mustache": "^4.0.1"
  }
}
