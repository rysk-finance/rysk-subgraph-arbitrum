{
  "name": "rysk-subgraph",
  "version": "1.3.4",
  "license": "UNLICENSED",
  "scripts": {
    "prepare:local": "mustache config/arbitrum-local.json subgraph.template.yaml > subgraph.yaml",
    "build:local": "yarn prepare:local && graph codegen && graph build",
    "deploy:local": "scripts/deploy.sh arbitrum-local",
    "deploy:local:overwrite": "scripts/deploy.sh arbitrum-local --overwrite",
    "prepare:goerli": "mustache config/arbitrum-goerli.json subgraph.template.yaml > subgraph.yaml",
    "build:goerli": "yarn prepare:goerli && graph codegen && graph build",
    "deploy:goerli": "scripts/deploy.sh arbitrum-goerli --remove-graft",
    "prepare:arbitrum": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
    "build:arbitrum": "yarn prepare:arbitrum && graph codegen && graph build",
    "deploy:arbitrum": "scripts/deploy.sh arbitrum-one --remove-graft",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.31.0"
  },
  "devDependencies": {
    "@goldskycom/cli": "^2.4.1",
    "@graphprotocol/graph-cli": "^0.58.0",
    "dotenv": "^16.3.1",
    "matchstick-as": "^0.5.2",
    "mustache": "^4.2.0"
  }
}
